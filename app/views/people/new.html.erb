<h1>People#new</h1>
<%#= devise_error_messages! %>
<%= form_for @person do |p| %>
  <%= p.label :pseudo %>
  <%= p.text_field :pseudo %>
  <%= fields_for @user do |u| %>
    <%= render partial: "devise/registrations/form", locals: { f: u } %>
  <% end %>
  <%= p.submit "Submit" %>
<% end %>

<%#= render "devise/shared/links" %>
